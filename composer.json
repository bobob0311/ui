{
    "name": "jiny/ui",
    "description": "jiny ui components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\UI\\":"src/",
            "jiny/html": "dev-master"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\UI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\UI\\JinyUIServiceProvider"
            ]
        }
    }

}
